                            <Label Content="Minimum work Duration for Week" Height="29" VerticalAlignment="Top" Grid.Column="4" Foreground="Black" HorizontalAlignment="Left" Width="195" Grid.ColumnSpan="2" Grid.RowSpan="2" Margin="45,0,0,0"/>
                            <Label Content="Minimum work Duration for Month" Height="29" VerticalAlignment="Top" Grid.Column="4" Foreground="Black" HorizontalAlignment="Left" Width="200" Grid.ColumnSpan="2" Grid.RowSpan="2" Margin="45,12,0,0" Grid.Row="1"/>
                            <xctk:MaskedTextBox ToolTip="DD.HH:MM" Mask="00.00:00" HorizontalAlignment="Left" Margin="123,-1,0,0" TextWrapping="Wrap" Text="{Binding OpenShiftMinWordDurationforWeek, StringFormat=\{0:dd\'.\'hh\':\'mm\}, ValidatesOnDataErrors=True}" IsEnabled="{Binding IsOpenShift}" Validation.ErrorTemplate="{StaticResource ErrorTemplate}" VerticalAlignment="Top" Width="83" Height="23" Grid.Column="5" Grid.RowSpan="2"/>
                            <xctk:MaskedTextBox ToolTip="DD.HH:MM" Mask="00.00:00" HorizontalAlignment="Left" Margin="123,12,0,0" TextWrapping="Wrap" Text="{Binding OpenShiftMinWordDurationforMonth, StringFormat=\{0:dd\'.\'hh\':\'mm\}, ValidatesOnDataErrors=True}" IsEnabled="{Binding IsOpenShift}" Validation.ErrorTemplate="{StaticResource ErrorTemplate}" VerticalAlignment="Top" Width="83" Height="23" Grid.Column="5" Grid.RowSpan="2" Grid.Row="1"/>

                        </Grid>
                    </Grid>
                </AdornerDecorator>
            </TabItem>
        </TabControl>
        <Button Command="{Binding NewButton}" Content="New" HorizontalAlignment="Left" Margin="24,19,0,0" VerticalAlignment="Top" Width="65" Height="25" BorderBrush="{x:Null}" Grid.Row="6" Grid.Column="8" Foreground="Black" Grid.ColumnSpan="3" Grid.RowSpan="2"/>
        <Button Command="{Binding SaveButton}" Content="Save" HorizontalAlignment="Left" Margin="27,19,0,0" VerticalAlignment="Top" Width="65" Height="25" BorderBrush="{x:Null}" Grid.Row="6" Grid.Column="10" Grid.RowSpan="2" Grid.ColumnSpan="2"/>
        <Button Command="{Binding DeleteButton}" Content="Delete" Margin="26,19,0,6" BorderBrush="{x:Null}" HorizontalAlignment="Left" Width="65" Grid.Row="6" Grid.Column="11" Height="25" Grid.RowSpan="2" Grid.ColumnSpan="2" />
        <ComboBox ItemsSource="{Binding ShiftCategories}" SelectedItem="{Binding CurrentShiftCategory}" DisplayMemberPath="shift_category_name" SelectedValuePath="shift_category_id" Grid.Column="4" HorizontalAlignment="Left" Margin="0,4,0,0" Grid.Row="2" VerticalAlignment="Top" Width="170" Height="23"/>
        <Label Content="Shifts" Margin="41,1,0,2" FontSize="13.333" Foreground="White" Grid.Column="7" Grid.Row="2" Grid.ColumnSpan="2"/>
        <ComboBox ItemsSource="{Binding Shifts}" SelectedItem="{Binding CurrentShift}" DisplayMemberPath="shift_detail_name" SelectedValuePath="shift_detail_id" Grid.Column="9" HorizontalAlignment="Left" Margin="17,7,0,0" Grid.Row="2" VerticalAlignment="Top" Width="166" Height="23" Grid.ColumnSpan="4"/>
    </Grid>
</Window>