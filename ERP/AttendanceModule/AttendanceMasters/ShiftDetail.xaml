                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0*"/>
                            <ColumnDefinition Width="29*"/>
                            <ColumnDefinition Width="132*"/>
                            <ColumnDefinition Width="142*"/>
                            <ColumnDefinition Width="42*"/>
                            <ColumnDefinition Width="91*"/>
                            <ColumnDefinition Width="42*"/>
                            <ColumnDefinition Width="121*"/>
                            <ColumnDefinition Width="21*"/>
                            <ColumnDefinition Width="30*"/>
                            <ColumnDefinition Width="0*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0*"/>
                            <RowDefinition Height="10*"/>
                            <RowDefinition Height="27*"/>
                            <RowDefinition Height="5*"/>
                            <RowDefinition Height="27*"/>
                            <RowDefinition Height="5*"/>
                            <RowDefinition Height="27*"/>
                            <RowDefinition Height="5*"/>
                            <RowDefinition Height="27*"/>
                            <RowDefinition Height="5*"/>
                            <RowDefinition Height="27*"/>
                            <RowDefinition Height="10*"/>
                            <RowDefinition Height="0*"/>
                        </Grid.RowDefinitions>
                        <Label Content="Shift In Day" Margin="0,19,55,2" Foreground="Black" Grid.Row="2" Grid.Column="2" Grid.RowSpan="2"/>
                        <Label Content="Shift Out Day" Margin="1,19,41,2" Foreground="Black" Grid.Row="2" Grid.Column="5" Grid.RowSpan="2" Grid.ColumnSpan="2"/>
                        <Label Content="Shift In Time" Margin="0,18,50,2" Foreground="Black" Grid.Row="4" Grid.Column="2" Grid.RowSpan="2"/>
                        <Label Content="Shift Out Time" Margin="1,19,41,2" Foreground="Black" Grid.Row="4" Grid.Column="5" Grid.RowSpan="2" Grid.ColumnSpan="2"/>
                        <Label Content="Shift On Duration" Margin="0,19,0,2" Foreground="Black" Grid.Row="6" Grid.Column="2" Grid.RowSpan="2"/>
                        <Label Content="Shift Off Duration" Margin="1,19,0,2" Foreground="Black" Grid.Row="6" Grid.Column="5" Grid.RowSpan="2" Grid.ColumnSpan="2"/>
                        <ComboBox ItemsSource="{Binding ShiftDayRanges}" IsEnabled="False" SelectedItem="{Binding ShiftInDayRange}" DisplayMemberPath="day_description" SelectedValuePath="day_value" SelectedValue="{Binding ShiftInDayValue}" Height="23" Grid.Column="3" Margin="0,22,0,2" Grid.Row="2" Grid.RowSpan="2" />
                        <xctk:MaskedTextBox ToolTip="HH:MM" Mask="00:00" HorizontalAlignment="Left" Margin="0,19,0,0" TextWrapping="Wrap" Text="{Binding ShiftInTime, Mode=TwoWay, StringFormat=\{0:hh\':\'mm\}, ValidatesOnDataErrors=True}" Validation.ErrorTemplate="{StaticResource ErrorTemplate}" VerticalAlignment="Top" Width="142" Height="23" Grid.Column="3" Grid.Row="4" Grid.RowSpan="2"/>
                        <ComboBox ItemsSource="{Binding ShiftDayRanges}" SelectedItem="{Binding ShiftOutDayRange}" DisplayMemberPath="day_description" SelectedValuePath="day_value" SelectedValue="{Binding ShiftOutDayValue}" Validation.ErrorTemplate="{StaticResource ErrorTemplate}" Height="23" Grid.Column="7" Margin="0,22,0,2" Grid.Row="2" Grid.RowSpan="2" Grid.ColumnSpan="2" />
                        <xctk:MaskedTextBox ToolTip="HH:MM" Mask="00:00" HorizontalAlignment="Left" Margin="0,22,0,0" TextWrapping="Wrap" Text="{Binding ShiftOutTime, StringFormat=\{0:hh\':\'mm\}, ValidatesOnDataErrors=True}" VerticalAlignment="Top" Width="142" Height="23" Grid.Column="7" Grid.Row="4" Grid.RowSpan="2" Grid.ColumnSpan="2"/>
                        <xctk:MaskedTextBox ToolTip="DD.HH:MM" Mask="00.00:00" HorizontalAlignment="Left" Margin="0,22,0,0" TextWrapping="Wrap" Text="{Binding ShiftOnTimeDuration, StringFormat=\{0:dd\'.\'hh\':\'mm\}, ValidatesOnDataErrors=True}" IsEnabled="{Binding IsBoolConvert}" Validation.ErrorTemplate="{StaticResource ErrorTemplate}" VerticalAlignment="Top" Width="142" Height="23" Grid.Column="3" Grid.Row="6" Grid.RowSpan="2"/>
                        <xctk:MaskedTextBox ToolTip="DD.HH:MM" Mask="00.00:00" Text="{Binding ShiftOffTimeDuration, StringFormat=\{0:dd\'.\'hh\':\'mm\},ValidatesOnDataErrors=True}" Validation.ErrorTemplate="{StaticResource ErrorTemplate}" HorizontalAlignment="Left" Margin="0,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="142" Height="23" Grid.Column="7" Grid.Row="6" Grid.RowSpan="2" Grid.ColumnSpan="2"/>
                        <RadioButton IsChecked="{Binding IsDailyShift}" Content="Daily Shift" GroupName="shiftType" Grid.Column="1" HorizontalAlignment="Left" Margin="28,6,0,0" Grid.Row="10" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
                        <RadioButton IsChecked="{Binding IsRosterShift}" Content="Roster" GroupName="shiftType" Grid.Column="2" HorizontalAlignment="Left" Margin="92,6,0,0" Grid.Row="10" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
                        <CheckBox IsChecked="{Binding IsSplitShift}" Content="Split Shift" HorizontalAlignment="Left" VerticalAlignment="Top" Height="15" Width="79" HorizontalContentAlignment="Center" Margin="0,27,0,0" Grid.Column="2" Grid.Row="10" Grid.RowSpan="2" />
                        <CheckBox IsChecked="{Binding IsEntitleLeiuLeave}" Content="Entitle Leiu Leave" HorizontalAlignment="Left" Width="121" HorizontalContentAlignment="Center" Margin="43,21,0,6" Grid.Column="5" Grid.Row="8" Grid.ColumnSpan="3" Grid.RowSpan="2" />
                        <CheckBox Visibility="Collapsed" IsChecked="{Binding IsNextDayOTPreDefined}" Content="Next Day OT&#xD;&#xA;Pre Defined" HorizontalAlignment="Left" VerticalAlignment="Top" Height="41" Width="110" HorizontalContentAlignment="Center" Margin="87,27,0,-13" Grid.Column="3" Grid.Row="10" Grid.ColumnSpan="3" Grid.RowSpan="3" />
                        <RadioButton IsChecked="{Binding IsExecutive}" IsEnabled="{Binding IsEntitleLeiuLeave}" Content="Executive" GroupName="shiftCat" Grid.Column="5" HorizontalAlignment="Left" Margin="61,3,0,0" Grid.Row="9" VerticalAlignment="Top" Grid.RowSpan="2" Grid.ColumnSpan="2"/>
                        <RadioButton IsChecked="{Binding IsNonExecutive}" IsEnabled="{Binding IsEntitleLeiuLeave}" Content="Non-Executive" GroupName="shiftCat" Grid.Column="5" HorizontalAlignment="Left" Margin="61,16,0,0" Grid.Row="10" VerticalAlignment="Top" Grid.ColumnSpan="3"/>
                        <RadioButton IsChecked="{Binding IsSecurity}" IsEnabled="{Binding IsEntitleLeiuLeave}" Content="Security" GroupName="shiftCat" Grid.Column="5" HorizontalAlignment="Left" Margin="61,36,0,0" Grid.Row="10" VerticalAlignment="Top" Grid.RowSpan="2" Width="68" Grid.ColumnSpan="2"/>
                        <CheckBox IsChecked="{Binding IsOTShift}" Content="OT Shift" HorizontalAlignment="Left" Width="81" HorizontalContentAlignment="Center" Margin="49,21,0,6" Grid.Column="7" Grid.Row="8" Grid.RowSpan="2" Grid.ColumnSpan="2" />
                        <RadioButton IsChecked="{Binding IsSingleOT}" IsEnabled="{Binding IsOTShift}" Content="Single OT" GroupName="otType" Grid.Column="7" HorizontalAlignment="Left" Margin="67,3,0,0" Grid.Row="9" VerticalAlignment="Top" Grid.RowSpan="2" Grid.ColumnSpan="2"/>
                        <RadioButton IsChecked="{Binding IsMultipleOT}" IsEnabled="{Binding IsOTShift}" Content="Multiple OT" GroupName="otType" Grid.Column="7" HorizontalAlignment="Left" Margin="67,16,0,0" Grid.Row="10" VerticalAlignment="Top" Grid.ColumnSpan="3"/>
                        <CheckBox IsChecked="{Binding IsHalfDay}" Content="HalfDay Shift" HorizontalAlignment="Left" VerticalAlignment="Top" Height="15" Width="110" HorizontalContentAlignment="Center" Margin="92,27,0,0" Grid.Column="2" Grid.Row="10" Grid.ColumnSpan="2" Grid.RowSpan="2" />
                        <CheckBox IsChecked="{Binding IsOpenShift}"  Content="Open Shift" HorizontalAlignment="Left" VerticalAlignment="Top" Height="15" Width="79" HorizontalContentAlignment="Center" Margin="3,0,0,0" Grid.Column="2" Grid.Row="2" />
                        <CheckBox IsChecked="{Binding IsSpecShift}" Content="Special Shift" HorizontalAlignment="Left" Width="92" HorizontalContentAlignment="Center" Margin="1,21,0,6" Grid.Column="2" Grid.Row="8" Grid.RowSpan="2" />
                        <CheckBox IsChecked="{Binding IsMorIncentive}" Content="Morning Incentive" HorizontalAlignment="Left" Width="119" HorizontalContentAlignment="Center" Margin="98,21,0,6" Grid.Column="2" Grid.Row="8" Grid.RowSpan="2" Grid.ColumnSpan="2" />
                    </Grid>
                </AdornerDecorator>
            </TabItem>
            <TabItem Header="Late In Details" IsEnabled="{Binding IsBoolConvert}" >
                <AdornerDecorator>
                    <Grid Height="253.873" Width="650">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0*"/>
                            <RowDefinition/>
                            <RowDefinition Height="0*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0*"/>
                            <ColumnDefinition Width="163*"/>
                            <ColumnDefinition Width="162*"/>
                            <ColumnDefinition Width="0*"/>
                        </Grid.ColumnDefinitions>
                        <Grid Grid.ColumnSpan="3" HorizontalAlignment="Left" Height="230" Margin="25,10,0,0" Grid.RowSpan="2" VerticalAlignment="Top" Width="538.667">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="7*"/>
                                <RowDefinition Height="15*"/>
                                <RowDefinition Height="5*"/>
                                <RowDefinition Height="15*"/>
                                <RowDefinition Height="5*"/>
                                <RowDefinition Height="15*"/>
                                <RowDefinition Height="5*"/>
                                <RowDefinition Height="15*"/>
                                <RowDefinition Height="5*"/>
                                <RowDefinition Height="15*"/>
                                <RowDefinition Height="2*"/>
                                <RowDefinition Height="15*"/>

                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="26*"/>
                                <ColumnDefinition Width="128*"/>
                                <ColumnDefinition Width="291*"/>
                                <ColumnDefinition Width="54*"/>
                            </Grid.ColumnDefinitions>
                            <Label Content="Late In Duration" HorizontalAlignment="Left" Height="26" Margin="0.621,2.17,0,0" VerticalAlignment="Top" Width="105" Grid.Row="5" Grid.Column="1" Foreground="Black"/>
                            <Label Content="Grace Remain" HorizontalAlignment="Left" Height="26" Margin="1,1.336,0,0" VerticalAlignment="Top" Width="105" Grid.Row="3" Grid.Column="1" Foreground="Black"/>
                            <Label Content="Grace Effect Duration" HorizontalAlignment="Left" Height="26" Margin="0.621,1.168,0,0" VerticalAlignment="Top" Width="123.112" Grid.Column="1" Foreground="Black" Grid.Row="1"/>
                            <Label Content="Short Leave Duration" HorizontalAlignment="Left" Height="26" Margin="1,2,0,0" VerticalAlignment="Top" Width="121" Grid.Row="7" Grid.Column="1" Foreground="Black"/>
                            <Label Content="Half Day Duration" HorizontalAlignment="Left" Height="26.668" Margin="1,0,0,0" VerticalAlignment="Top" Width="104" Grid.Row="9" Grid.Column="1" Foreground="Black"/>
                            <Label Visibility="Collapsed" Content="Full Day Duration" HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Width="105" Grid.Row="11" Grid.Column="1" Foreground="Black" Margin="1,1,0,0"/>
                            <xctk:MaskedTextBox ToolTip="DD.HH:MM" Mask="00.00:00" Text="{Binding LateGraceEffectDuration, StringFormat=\{0:dd\'.\'hh\':\'mm\}, ValidatesOnDataErrors=True}" Validation.ErrorTemplate="{StaticResource ErrorTemplate}" HorizontalAlignment="Left" Margin="2.122,2.668,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="115" Height="24" Grid.Column="2" Grid.Row="1"/>
                            <xctk:MaskedTextBox ToolTip="DD.HH:MM" Mask="00.00:00" Text="{Binding LateGraceTimeDuration, StringFormat=\{0:dd\'.\'hh\':\'mm\}, ValidatesOnDataErrors=True}" Validation.ErrorTemplate="{StaticResource ErrorTemplate}" HorizontalAlignment="Left" Margin="2.001,2.5,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="115" Height="24" Grid.Column="2" Grid.Row="3"/>
                            <xctk:MaskedTextBox ToolTip="DD.HH:MM" Mask="00.00:00" Text="{Binding LateInTimeDuration, StringFormat=\{0:dd\'.\'hh\':\'mm\}, ValidatesOnDataErrors=True}" Validation.ErrorTemplate="{StaticResource ErrorTemplate}" HorizontalAlignment="Left" Margin="2.122,2.504,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="115" Height="24" Grid.Column="2" Grid.Row="5"/>
                            <xctk:MaskedTextBox ToolTip="DD.HH:MM" Mask="00.00:00" Text="{Binding LateShotLeaveTimeDuration, StringFormat=\{0:dd\'.\'hh\':\'mm\}, ValidatesOnDataErrors=True}" Validation.ErrorTemplate="{StaticResource ErrorTemplate}" HorizontalAlignment="Left" Margin="2.334,2,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="115" Height="24" Grid.Column="2" Grid.Row="7"/>
                            <xctk:MaskedTextBox ToolTip="DD.HH:MM" Mask="00.00:00" Text="{Binding LateHalfDayTimeDuration, StringFormat=\{0:dd\'.\'hh\':\'mm\}, ValidatesOnDataErrors=True}" Validation.ErrorTemplate="{StaticResource ErrorTemplate}" HorizontalAlignment="Left" Margin="1.834,1.668,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="115" Height="24" Grid.Column="2" Grid.Row="9"/>
                            <xctk:MaskedTextBox Visibility="Collapsed" Mask="00.00:00" Text="{Binding LateFullLeaveTimeDuration, StringFormat=\{0:dd\'.\'hh\':\'mm\}, ValidatesOnDataErrors=True}" HorizontalAlignment="Left" Margin="1,1,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="115" Height="24" Grid.Column="2" Grid.Row="11"/>
                            <Label Content="{Binding LateGraceEffectTime}" ContentStringFormat="yyyy/MM/dd hh:mm:ss tt" HorizontalAlignment="Left" VerticalAlignment="Top" Height="24" Width="150" Grid.Column="2" Margin="213,3,0,0" Grid.Row="1" Grid.ColumnSpan="2" Background="#FFD1D1D1"/>
                            <Label Content="{Binding LateGraceRemainTime}" ContentStringFormat="yyyy/MM/dd hh:mm:ss tt" HorizontalAlignment="Left" VerticalAlignment="Top" Height="24" Width="150" Grid.Column="2" Margin="213,3,0,0" Grid.Row="3" Grid.ColumnSpan="2" Background="#FFD1D1D1"/>
