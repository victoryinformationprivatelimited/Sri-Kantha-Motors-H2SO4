                        </Grid>
                    </Grid>
                </AdornerDecorator>
            </TabItem>
            <TabItem Header="Early Out Details" IsEnabled="{Binding IsBoolConvert}" >
                <AdornerDecorator>
                    <Grid Height="253.873" Width="650">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0*"/>
                            <RowDefinition/>
