        <Label Content="   ETF" HorizontalAlignment="Left" Margin="0.036,0.145,0,0" Grid.Row="3" VerticalAlignment="Top" Width="58" FontSize="16" Background="{x:Null}" Height="28" Grid.RowSpan="2" Grid.Column="1"/>
        <TextBox Grid.Row="3" TextWrapping="Wrap" Height="23" VerticalAlignment="Top" Grid.Column="3" HorizontalAlignment="Left" Width="130.468"/>
        <Label Content="%" Grid.Column="4" Margin="0,0.145,31.23,0" Grid.Row="3" FontSize="14" Background="{x:Null}" HorizontalAlignment="Right" Width="23" Height="27.003" VerticalAlignment="Top" Grid.RowSpan="2"/>
        <Label Content="   EPF" HorizontalAlignment="Left" Margin="1,0.623,0,0" Grid.Row="5" VerticalAlignment="Top" Width="58" FontSize="16" Background="{x:Null}" Height="28.69" Grid.Column="1" Grid.RowSpan="2"/>
        <TextBox Grid.Row="5" TextWrapping="Wrap" Height="23" VerticalAlignment="Top" Grid.Column="3"/>
        <Label Content="%" Grid.Column="4" Margin="0,3.313,27.668,0" Grid.Row="5" FontSize="14" Background="{x:Null}" HorizontalAlignment="Right" Width="23" Height="26" VerticalAlignment="Top" Grid.RowSpan="2"/>
        <Button Content="Save" Margin="65.468,4.466,64.229,-0.001" Grid.Row="9" Grid.Column="3" Width="65" Grid.ColumnSpan="2" Grid.RowSpan="2" Height="20"/>
        <Label Content="Payee Tax" HorizontalAlignment="Left" Margin="10,0,0,0" Grid.Row="7" VerticalAlignment="Top" Height="25.96" Width="64.266" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
