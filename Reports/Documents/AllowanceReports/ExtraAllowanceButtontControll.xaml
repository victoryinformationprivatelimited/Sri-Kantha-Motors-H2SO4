                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </CheckBox.RenderTransform>
                </CheckBox>
            </WrapPanel>
        </WrapPanel>
        <WrapPanel x:Name="DetailFilteringMDI" Margin="412,0,0,0" RenderTransformOrigin="0.5,0.5"/>

    </Grid>
</UserControl>
